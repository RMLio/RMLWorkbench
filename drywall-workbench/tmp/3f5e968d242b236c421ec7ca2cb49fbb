# CULTALGENIEUW3-1.csv

<#cultalgem-loc-pom> rr:predicate ewins:location;
        rr:objectMap [
	    rr:parentTriplesMap <#Gemeentes>;
            rr:joinCondition [
		rr:child "gemeente";
		rr:parent "Gemeente"
            ]
        ] .

<#cultalgem-time-pom> 	rr:predicate prov:generatedAtTime;
    	rr:objectMap [
        	rr:constant "2011-01-01T00:00:00";
        	rr:datatype xsd:dateTime
    	] .

<#cultalgem-logsource>   rml:sourceName "CULTALGENIEUW3-1-xls.csv";
        rml:queryLanguage ql:CSV .



