@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix mail: <http://example.com/mail#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ex: <http://www.example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix transit: <http://vocab.org/transit/terms/> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.
@prefix gn: <http://www.geonames.org/ontology#>.
@prefix gs: <http://www.opengis.net/ont/geosparql/>.
@prefix geosp: <http://www.telegraphis.net/ontology/geography/geography#> .
@prefix ewins:  <http://ewi.mmlab.be/ns#> .
@prefix ewi:    <http://ewi.mmlab.be/geo#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<#Terms>
    rml:logicalSource [
        rml:sourceName "terms.csv";
        rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
        rml:reference "URI";
        rr:class ewins:Statistic
    ];
    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
        rml:reference "Label"
        ]
    ];
    rr:predicateObjectMap [
    rr:predicate ewins:inCategory;
    rr:objectMap [
        rr:parentTriplesMap <#Terms2>;
        ]
    ].
    
<#Terms2>
    rml:logicalSource [
        rml:sourceName "terms.csv";
        rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
        rml:reference "Category URI";
#    rr:templateMap [
#            rml:reference "Category URI";
#         ];
     rr:class ewins:StatisticCategory
    ];
    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
        rml:reference "Category Label"
        ]
    ].

  
<#Gemeentes>
    rml:logicalSource [
    rml:sourceName "gemeentecodes.csv";
        rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
     rr:template "http://ewi.mmlab.be/datajournalism/gemeentes/{NIS code}";
     rr:class schema:City, gn:Feature
    ];
    rr:predicateObjectMap [
    rr:predicate gn:postalCode;
    rr:objectMap [
        rml:reference "Postcode"
        ]
    ];  
    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
        rml:reference "Gemeente"
        ]
    ];
    rr:predicateObjectMap [
    rr:predicate gn:locatedIn;
    rr:objectMap [
        rr:template "http://ewi.mmlab.be/datajournalism/provincies/{Provincie}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate gs:hasGeometry;
        rr:objectMap [
            rr:parentTriplesMap <#geo>;
            rr:joinCondition [
            rr:child "NIS code";
            #was: rr:parent "gml:featureMember/agiv:Refgem/agiv:NISCODE";
                        rr:parent "agiv:Refgem/agiv:NISCODE";
            ]
        ] 
    ] .

<#geo>
    rml:logicalSource [
      rml:sourceName "Refgem.gml";
      rml:iterator "/agiv:FeatureCollection/gml:featureMember";
      rml:queryLanguage ql:XPath;
    ];
    
    rr:subjectMap [
      rr:template "http://ewi.mmlab.be/datajournalism/geo/{agiv:Refgem/agiv:GEOMID}";
      rr:class gs:Geometry 
    ];
  
    rr:predicateObjectMap [
        rr:predicate gs:asGML;
        rr:objectMap [
            rml:reference "agiv:Refgem/gml:polygonProperty/gml:Polygon";
            rr:datatype <http://www.opengis.net/ont/geosparql#gmlLiteral>
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate gs:asGML;
        rr:objectMap [
            rml:reference "agiv:Refgem/gml:multiPolygonProperty/gml:MultiPolygon";
            rr:datatype <http://www.opengis.net/ont/geosparql#gmlLiteral>
        ]
    ] .

<#geo2>
    rml:logicalSource [
      rml:sourceName "Refprv.gml";
      rml:iterator "/agiv:FeatureCollection/gml:featureMember";
      rml:queryLanguage ql:XPath;
    ];
    
    rr:subjectMap [
      rr:template "http://ewi.mmlab.be/datajournalism/geo/{agiv:Refprv/agiv:ATTRGEOMID}";
      rr:class gs:Geometry 
    ];
  
    rr:predicateObjectMap [
        rr:predicate gs:asGML;
        rr:objectMap [
            rml:reference "agiv:Refprv/gml:polygonProperty/gml:Polygon";
            rr:datatype <http://www.opengis.net/ont/geosparql#gmlLiteral>
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate gs:asGML;
        rr:objectMap [
            rml:reference "agiv:Refprv/gml:multiPolygonProperty/gml:MultiPolygon";
            rr:datatype <http://www.opengis.net/ont/geosparql#gmlLiteral>
        ]
    ] .

<#geo3>
    rml:logicalSource [
      rml:sourceName "Refarr.gml";
      rml:iterator "/agiv:FeatureCollection/gml:featureMember";
      rml:queryLanguage ql:XPath;
    ];
    
    rr:subjectMap [
      rr:template "http://ewi.mmlab.be/datajournalism/geo/{agiv:Refarr/agiv:ATTRGEOMID}";
      rr:class gs:Geometry 
    ];
  
    rr:predicateObjectMap [
        rr:predicate gs:asGML;
        rr:objectMap [
            rml:reference "agiv:Refarr/gml:polygonProperty/gml:Polygon";
            rr:datatype <http://www.opengis.net/ont/geosparql#gmlLiteral>
        ]
    ] .

<#geo3-arrondissementen>
    rml:logicalSource [
      rml:sourceName "Refarr.gml";
      rml:iterator "/agiv:FeatureCollection/gml:featureMember";
      rml:queryLanguage ql:XPath;
    ];
    
    rr:subjectMap [
      rr:template "http://ewi.mmlab.be/datajournalism/arrondissementen/{agiv:Refarr/agiv:NAAM}";
      rr:class  ewins:District, gn:Feature
    ];

    rr:predicateObjectMap [
        rr:predicate gs:hasGeometry;
        rr:objectMap [
            rr:parentTriplesMap <#geo3>;
        ] 
    ];
  
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
            rml:reference "agiv:Refarr/agiv:NAAM";
        ]
    ] .



<#Provincies>
    rml:logicalSource [
    rml:sourceName "gemeentecodes.csv";
        rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
     rr:template "http://ewi.mmlab.be/datajournalism/provincies/{Provincie}";
     rr:class schema:State, gn:Feature
    ];
    rr:predicateObjectMap [
    rr:predicate gn:locatedIn;
    rr:objectMap [
        rr:template "http://ewi.mmlab.be/datajournalism/gewesten/{Gewest}"
        ]
    ];
    rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
        rml:reference "Provincie"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate gs:hasGeometry;
        rr:objectMap [
            rr:parentTriplesMap <#geo2>;
            rr:joinCondition [
            rr:child "Provincie";
                        rr:parent "agiv:Refprv/agiv:NAAM";
            ]
        ] 
    ] .

    
#
# Statistieken
#

<#timeMap> 
    rr:predicate prov:generatedAtTime;
    rr:objectMap [
        rr:template "{Jaar}-01-01T00:00:00";
        rr:datatype xsd:dateTime
    ] .

<#provincieMap>
        rr:predicate ewins:location;
        rr:objectMap [
            rr:template "http://ewi.mmlab.be/datajournalism/provincies/{Provincie}"
        ] .


    
#1_Bevolking_prov-csv.csv
#-----------------------------------------------------------------------------------
    
<#provincie-population-datapoint>
    rml:logicalSource [
        rml:sourceName "1_Bevolking_prov-csv.csv";
        rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://ewi.mmlab.be/datajournalism/provincies/{Provincie}/bevolking/{Jaar}";
        rr:class ewins:Datapoint
        
    ];
    rr:predicateObjectMap [
        rr:predicate ewins:datapointFor;
        rr:object ewi:Population
    ];
    rr:predicateObjectMap [
        rr:predicate ewins:value;
        rr:objectMap [
            rml:reference "Bevolking";
            rr:datatype xsd:int
        ]
    ];
    rr:predicateObjectMap <#timeMap>;
    rr:predicateObjectMap <#provincieMap>.
    
